class Account {
	
	String accountNo;
	String ownerName;
	int balance;
	
	public Account() {
		
		accountNo = "111-1111-111";
		ownerName = "홍길동";
		balance = 0;
		
	}
	
	public Account(String newAccountNo) {
		
		ownerName = "홍길동";
		balance = 0;
		
	}
	
}