
public class Test2 {

	public static void main(String[] args) {

		int min = 0;
		int sec = 0;
		
		while(min <= 59) {
			sec = 0;
			while(sec <= 59) {
				System.out.println(min + "분 " + sec + "초");
				sec++;
			}
			min++;
		}
		
		System.out.println("===========================");
		
		int dan = 1;
		int j = 1;
		
		while(dan <= 9) {
			System.out.println(dan + "단");
			
			j = 1;
			
			while (j < 10) {
				System.out.println(dan + " * " + j + " = " + dan * j);
				j++;
			}
			dan++;
		}
		
	}
}

