
public class Test2 {

	public static void main(String[] args) {

		int min = 0;
		int sec = 0;
		
		while(min <= 59) {
			sec = 0;
			while(sec <= 59) {
				System.out.println(min + "분 " + sec + "초");
				sec++;
			}
			min++;
		}
		
		System.out.println("===========================");
		
		int i = 0;
		int j = 0;
		
	}
}

